<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/secondary_background"
    android:foreground="?attr/selectableItemBackground"
    tools:ignore="RtlHardcoded,RtlSymmetry">

    <TextView
        android:id="@+id/tv_series_date"
        android:layout_width="wrap_content"
        android:layout_height="48dp"
        android:layout_gravity="center_vertical|left"
        android:alpha="0.87"
        android:gravity="center"
        android:paddingLeft="@dimen/list_key_line"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textColor="@android:color/black" />

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_vertical|right">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tv_series_game_5"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:alpha="0.54"
                android:gravity="center"
                android:paddingBottom="@dimen/match_play_text_padding_2"
                android:paddingLeft="@dimen/match_play_text_padding"
                android:paddingRight="@dimen/match_play_text_padding"
                android:paddingTop="@dimen/match_play_text_padding"
                android:textColor="@android:color/black"
                android:textSize="16sp"
                tools:ignore="RelativeOverlap" />

            <TextView
                android:id="@+id/tv_series_match_play_5"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:alpha="0.54"
                android:gravity="center"
                android:paddingBottom="@dimen/match_play_text_padding"
                android:paddingLeft="@dimen/match_play_text_padding"
                android:paddingRight="@dimen/match_play_text_padding"
                android:paddingTop="@dimen/match_play_text_padding_2"
                android:textColor="@android:color/black"
                android:textSize="16sp"
                tools:ignore="RelativeOverlap" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tv_series_game_4"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:alpha="0.54"
                android:gravity="center"
                android:paddingBottom="@dimen/match_play_text_padding_2"
                android:paddingLeft="@dimen/match_play_text_padding"
                android:paddingRight="@dimen/match_play_text_padding"
                android:paddingTop="@dimen/match_play_text_padding"
                android:textColor="@android:color/black"
                android:textSize="16sp"
                tools:ignore="RelativeOverlap" />

            <TextView
                android:id="@+id/tv_series_match_play_4"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:alpha="0.54"
                android:gravity="center"
                android:paddingBottom="@dimen/match_play_text_padding"
                android:paddingLeft="@dimen/match_play_text_padding"
                android:paddingRight="@dimen/match_play_text_padding"
                android:paddingTop="@dimen/match_play_text_padding_2"
                android:textColor="@android:color/black"
                android:textSize="16sp"
                tools:ignore="RelativeOverlap" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tv_series_game_3"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:alpha="0.54"
                android:gravity="center"
                android:paddingBottom="@dimen/match_play_text_padding_2"
                android:paddingLeft="@dimen/match_play_text_padding"
                android:paddingRight="@dimen/match_play_text_padding"
                android:paddingTop="@dimen/match_play_text_padding"
                android:textColor="@android:color/black"
                android:textSize="16sp"
                tools:ignore="RelativeOverlap" />

            <TextView
                android:id="@+id/tv_series_match_play_3"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:alpha="0.54"
                android:gravity="center"
                android:paddingBottom="@dimen/match_play_text_padding"
                android:paddingLeft="@dimen/match_play_text_padding"
                android:paddingRight="@dimen/match_play_text_padding"
                android:paddingTop="@dimen/match_play_text_padding_2"
                android:textColor="@android:color/black"
                android:textSize="16sp"
                tools:ignore="RelativeOverlap" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tv_series_game_2"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:alpha="0.54"
                android:gravity="center"
                android:paddingBottom="@dimen/match_play_text_padding_2"
                android:paddingLeft="@dimen/match_play_text_padding"
                android:paddingRight="@dimen/match_play_text_padding"
                android:paddingTop="@dimen/match_play_text_padding"
                android:textColor="@android:color/black"
                android:textSize="16sp"
                tools:ignore="RelativeOverlap" />

            <TextView
                android:id="@+id/tv_series_match_play_2"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:alpha="0.54"
                android:gravity="center"
                android:paddingBottom="@dimen/match_play_text_padding"
                android:paddingLeft="@dimen/match_play_text_padding"
                android:paddingRight="@dimen/match_play_text_padding"
                android:paddingTop="@dimen/match_play_text_padding_2"
                android:textColor="@android:color/black"
                android:textSize="16sp"
                tools:ignore="RelativeOverlap" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/tv_series_game_1"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:alpha="0.54"
                android:gravity="center"
                android:paddingBottom="@dimen/match_play_text_padding_2"
                android:paddingLeft="@dimen/match_play_text_padding"
                android:paddingRight="@dimen/match_play_text_padding"
                android:paddingTop="@dimen/match_play_text_padding"
                android:textColor="@android:color/black"
                android:textSize="16sp"
                tools:ignore="RelativeOverlap" />

            <TextView
                android:id="@+id/tv_series_match_play_1"
                android:layout_width="wrap_content"
                android:layout_height="0dp"
                android:layout_gravity="center"
                android:layout_weight="1"
                android:alpha="0.54"
                android:gravity="center"
                android:paddingBottom="@dimen/match_play_text_padding"
                android:paddingLeft="@dimen/match_play_text_padding"
                android:paddingRight="@dimen/match_play_text_padding"
                android:paddingTop="@dimen/match_play_text_padding_2"
                android:textColor="@android:color/black"
                android:textSize="16sp"
                tools:ignore="RelativeOverlap" />

        </LinearLayout>

        <ImageView
            android:id="@+id/iv_edit_date"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical|right"
            android:layout_marginLeft="4dp"
            android:contentDescription="@string/description_edit_date"
            android:paddingRight="@dimen/list_key_line"
            android:src="@drawable/ic_edit_black_24dp" />

    </LinearLayout>

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_gravity="bottom"
        android:background="@color/divider" />

</FrameLayout>
